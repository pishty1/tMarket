{
  "name": "tmarket",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "account": "yarn workspace @tmarket/foundry account",
    "chain": "yarn workspace @tmarket/foundry chain",
    "compile": "yarn workspace @tmarket/foundry compile",
    "deploy": "yarn workspace @tmarket/foundry deploy",
    "deploy:verify": "yarn workspace @tmarket/foundry deploy:verify",
    "flatten": "yarn workspace @tmarket/foundry flatten",
    "fork": "yarn workspace @tmarket/foundry fork",
    "format": "yarn next:format && yarn foundry:format",
    "foundry:format": "yarn workspace @tmarket/foundry format",
    "foundry:lint": "yarn workspace @tmarket/foundry lint",
    "foundry:test": "yarn workspace @tmarket/foundry test",
    "generate": "yarn workspace @tmarket/foundry generate",
    "postinstall": "husky install",
    "next:build": "yarn workspace @tmarket/nextjs build",
    "next:check-types": "yarn workspace @tmarket/nextjs check-types",
    "next:format": "yarn workspace @tmarket/nextjs format",
    "next:lint": "yarn workspace @tmarket/nextjs lint",
    "next:serve": "yarn workspace @tmarket/nextjs serve",
    "precommit": "lint-staged",
    "start": "yarn workspace @tmarket/nextjs dev",
    "test": "yarn foundry:test",
    "vercel": "yarn workspace @tmarket/nextjs vercel",
    "vercel:yolo": "yarn workspace @tmarket/nextjs vercel:yolo",
    "verify": "yarn workspace @tmarket/foundry verify"
  },
  "devDependencies": {
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18.17.0"
  }
}
